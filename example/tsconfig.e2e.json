    "lib": [
      "es2017"
    ],
    "rootDir": "example/",
    "noEmit": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2016",
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
